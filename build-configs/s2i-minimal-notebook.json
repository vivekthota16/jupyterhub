{
    "kind": "List",
    "apiVersion": "v1",
    "items": [
        {
            "kind": "ImageStream",
            "apiVersion": "image.openshift.io/v1",
            "metadata": {
                "name": "s2i-minimal-notebook",
                "labels": {
                    "build": "s2i-minimal-notebook"
                }
            },
            "spec": {
                "lookupPolicy": {
                    "local": true
                }
            }
        },
        {
            "kind": "BuildConfig",
            "apiVersion": "build.openshift.io/v1",
            "metadata": {
                "name": "s2i-minimal-notebook-py35",
                "labels": {
                    "build": "s2i-minimal-notebook"
                }
            },
            "spec": {
                "triggers": [
                    {
                        "type": "ConfigChange"
                    },
                    {
                        "type": "ImageChange"
                    }
                ],
                "source": {
                    "type": "Git",
                    "git": {
                        "uri": "https://github.com/vivekthota16/jupyterhub"
                    },
                    "contextDir": "minimal-notebook"
                },
                "strategy": {
                    "type": "Source",
                    "sourceStrategy": {
                        "from": {
                            "kind": "ImageStreamTag",
                            "name": "python:3.5",
                            "namespace": "openshift"
                        }
                    }
                },
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "s2i-minimal-notebook:3.5"
                    }
                },
                "resources": {
                    "limits": {
                        "memory": "4Gi",
                        "cpu": 4,
                        "nvidia.com/gpu": 1
                    }
                }
            }
        },
        {
            "kind": "BuildConfig",
            "apiVersion": "build.openshift.io/v1",
            "metadata": {
                "name": "s2i-minimal-notebook-py36",
                "labels": {
                    "build": "s2i-minimal-notebook"
                }
            },
            "spec": {
                "triggers": [
                    {
                        "type": "ConfigChange"
                    },
                    {
                        "type": "ImageChange"
                    }
                ],
                "source": {
                    "type": "Git",
                    "git": {
                        "uri": "https://github.com/vivekthota16/jupyterhub"
                    },
                    "contextDir": "minimal-notebook"
                },
                "strategy": {
                    "type": "Source",
                    "sourceStrategy": {
                        "from": {
                            "kind": "ImageStreamTag",
                            "name": "docker-python3-ubuntu:3.8.0",
                            "namespace": "matthewfeickert"
                        }
                    }
                },
                "output": {
                    "to": {
                        "kind": "ImageStreamTag",
                        "name": "s2i-minimal-notebook:3.6"
                    }
                },
                "resources": {
                    "limits": {
                        "memory": "4Gi",
                        "cpu": 4,
                        "nvidia.com/gpu": 1
                    }
                }
            }
        }
    ]
}
